alias egrep     egrep --color
alias findd     'find . -type d -exec ls -ld {} \; | egrep -v "(drwx--S---)"'
alias findf     'find . -type f -exec ls -ld {} \; | egrep -v "(-rw.------)"'
alias findport  'find /usr/ports/ -maxdepth 2 -type d | egrep -i'
alias g         git
alias grep      grep --color
alias h         history 25
alias ioc       iocage
alias iftop     iftop -BPn
alias j         jobs -l
alias la        ls -aF
alias lf        ls -FA
alias ll        ls -lAF
alias nr        netstat -nr
alias omitblanklines    egrep -v '^\s*$'
alias omitcomments      egrep -v '^\s*#'
alias p         pkg-static
alias pg        pgrep -lf
alias squidlog  'sh -c "while [ 1 -eq 1 ]; do if [ -e /usr/jails/squid/var/log/squid/access.log ]; then tail -f /usr/jails/squid/var/log/squid/access.log ; fi ; sleep 3 ; done"'
alias sr        squidclient -m PURGE
alias stat-heat stat-temperature
alias ta        'tmux attach || tmux'
alias tas       tmux source-file ~/local/tmux.conf.status
alias tn        tmux new-window
alias top       top -CazHits1
alias zi        zpool import
alias zio       zpool iostat
alias zl        zfs list -o name,used,available,refer,usedbychildren,usedbysnapshots,quota,canmount,mounted,mountpoint,creation
alias zll       zfs list -o name,dedup,compressratio,compression,checksum,atime,exec,setuid,readonly,jailed,canmount,mounted,mountpoint
alias zls       zfs list -t snapshot -o name,used,refer,creation -s creation
alias zlss      zfs list -t snapshot -o name -s creation
alias zs        zpool status -v
