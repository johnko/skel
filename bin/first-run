#!/bin/sh

if [ "x" != "x${SQUID}" ]; then
    env SQUID=${SQUID} setproxy
fi

fres

cat <<EOF
Run these commands:
    fzg-random-key
    fzg -i
    fzg-unlock-on-boot
    exit
EOF
csh

eza install -h file:///boot/../10.1-RELEASE
eza-update

ssh-makeprivkey-ed25519

rm -r ~/bin/
sh ~/git/skel/bin/fres
